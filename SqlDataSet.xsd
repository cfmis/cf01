                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT a.within_code,a.id,a.ver,b.sequence_id,b.wp_id,a.mo_id,b.goods_id,c.name AS goods_name,c.blueprint_id,b.prod_qty,b.goods_unit,b.t_complete_date,a.customer_id,d.brand_id,e.production_remark,a.remark,b.next_wp_id,b.predept_rechange_qty,d.get_color_sample,d.order_qty,d.goods_unit AS order_unit,c.color,d.order_qty AS per_qty,0 AS page_num
,f.base_qty,f.unit_code,f.rate AS base_rate,f.basic_unit
,g.sequence_id AS art_id,g.picture_name,h.name AS color_name,h.do_color,(d.order_qty*i.rate) AS order_qty_pcs,j.name AS next_dep_name,h.do_color AS do_color1
		FROM jo_bill_mostly a
		INNER JOIN jo_bill_goods_details b ON a.within_code=b.within_code AND a.id=b.id AND a.ver=b.ver
		INNER JOIN it_goods c ON b.within_code=b.within_code AND b.goods_id=c.id
		INNER JOIN so_order_details d ON a.within_code=d.within_code AND a.order_no=d.id AND a.so_sequence_id=d.sequence_id AND a.so_ver=d.ver
		LEFT OUTER JOIN so_order_special_info e ON a.within_code=e.within_code AND a.order_no=e.id AND a.so_sequence_id=e.upper_sequence AND a.so_ver=e.ver
		LEFT OUTER JOIN it_coding f On b.within_code=f.within_code And b.goods_id=f.id AND b.goods_unit=f.basic_unit
                                LEFT OUTER JOIN cd_pattern_details g ON c.within_code=g.within_code AND c.blueprint_id=g.id
		LEFT OUTER JOIN cd_color h ON c.within_code=h.within_code AND c.color=h.id
		LEFT OUTER JOIN it_coding i On d.within_code=i.within_code And d.goods_unit=i.unit_code
		LEFT OUTER JOIN cd_department j ON b.within_code=j.within_code And b.next_wp_id=j.id</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
