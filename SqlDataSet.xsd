          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dtWork01TableAdapter" GeneratorDataComponentClassName="dtWork01TableAdapter" Name="dtWork01" UserDataComponentName="dtWork01TableAdapter">
